return b{
    $ hello.elf: hello.c
        riscv64-unknown-elf-gcc -march=rv64im -mabi=lp64 $input -Oz -o $output -T../../link/user.ld -ffreestanding -nostdlib
    $ hello.bin: hello.elf
        riscv64-unknown-elf-objcopy $input -O binary $output
    $ hello.list: hello.elf
        riscv64-unknown-elf-objdump -D $input > $output
}
